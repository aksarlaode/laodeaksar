{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^@aksar/"
      ],
      "enabled": false,
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    }
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "updateInternalDeps": true,
  "rangeStrategy": "bump"
}